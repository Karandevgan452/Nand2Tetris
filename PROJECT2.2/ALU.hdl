CHIP ALU {

    IN  // 16-bit inputs:
        x[16], y[16],
        

    OUT // 16-bit output
        out[16],

        // ALU output flags
        zr, // 1 if out=0, 0 otherwise
        ng; // 1 if out<0, 0 otherwise


}
